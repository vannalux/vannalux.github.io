          "_customHTML": "<section data-bs-version=\"5.1\" mbr-id=\"_anchor_product_slide\" class=\"mbr-section product_slide content1\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"8\" step=\"1\" value=\"0\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"8\" step=\"1\" value=\"2\">\n      <select inline title=\"Toggle Config\" name=\"addvatshipping2\">\n            <option value=\"1\">Show</option>\n            <option value=\"2\" selected>Hide</option>\n        </select>\n      <input type=\"range\" inline title=\"Slider No\" name=\"productno\" min=\"1\" max=\"4\" step=\"1\" value=\"3\">\n      <input inline type=\"text\" title=\"Currency Symbol\" name=\"CurrencyIcon\" value=\"$\"> \n        <input type=\"color\" title=\"Background Color\" name=\"bgColor\" value=\"#fafafa\">\n      <input type=\"color\" title=\"Card Bg Color\" name=\"cbgColor\" value=\"#7f1933\">\n      <input type=\"color\" title=\"Card Text Color\" name=\"ctbgColor\" value=\"#efefef\">\n    <!-- End block parameters -->\n    </mbr-parameters> \n\n    <section class=\"cont pt-5 pb-5\">\n  <div class=\"container\">\n    <div class=\"row\">\n        <div class=\"col-4\">\n            <span class=\"mb-0 slidehead\">Products&nbsp;</span>\n        </div>\n        <div class=\"col-8 text-right\">\n            <a class=\"btn btn-sm btn-primary mb-3 mr-0\" href=\"#carouselExampleIndicators2\" role=\"button\" data-slide=\"prev\" data-bs-slide=\"prev\">\n                <i class=\"fa fa-arrow-left fas\"></i>\n            </a>\n            <a class=\"btn btn-sm btn-primary mb-3 mr-0\" href=\"#carouselExampleIndicators2\" role=\"button\" data-slide=\"next\" data-bs-slide=\"next\">\n                <i class=\"fa fa-arrow-right fas\"></i>\n            </a>\n        </div>\n        <div class=\"col-12\">\n            <div id=\"carouselExampleIndicators2\" class=\"carousel slide\" data-ride=\"carousel\" data-bs-ride=\"carousel\">\n                <div class=\"carousel-inner\">\n                    <div class=\"carousel-item active\" mbr-if=\"productno > 0\">\n                        <div class=\"row\">\n                            <div class=\"col-md-4 mb-3\">\n                                <div class=\"card cart_item\">\n                                    <img class=\"img-fluid\" alt=\"100%x280\" src=\"https://i.imgur.com/JOpmFkw.png\">\n                                    <div class=\"card-body\">\n                                        <h4 class=\"card-title product_name\" mbr-article>Dell Xtreame 1</h4>\n                                        <p class=\"card-text\">If you want a different shop interface design, contact Mr Singa4real on +2349066849761</p>\n                                    </div> \n                                  <div class=\"group\" mbr-class=\"{'vatshipblock': addvatshipping2 == 1, 'vatshipnone': addvatshipping2 == 2}\">\n                             <div class=\"d-inline\"><strong>ID:</strong>&nbsp;&nbsp;</div>\n                            <div class=\"d-inline\"><strong><span class=\"product_id\" mbr-text>0011</span></strong></div>\n                            <div class=\"d-inline\">&nbsp;&nbsp;<strong>Vat:</strong>&nbsp;&nbsp;</div>\n                            <div class=\"d-inline\"><strong><span class=\"product_vat\" mbr-text>0</span></strong></div>\n                            <div class=\"d-inline\">&nbsp;&nbsp;<strong>Shipping:</strong>&nbsp;&nbsp;</div>\n                            <div class=\"d-inline\"><strong><span class=\"product_shipping\" mbr-text>0</span></strong></div>\n                          </div>\n                                  <div class=\"container row\">\n                                <div class=\"col-xs-12 col-md-12\">\n                                  <div class=\"row\">\n                                    <div class=\"col-xs-6 col-md-6  col-lg-6 col-6 lead\"><b><span html=\"{{CurrencyIcon}}\"></span><span class=\"product_price\" mbr-text>21,100</span></b></div> <div class=\"col-xs-6 col-md-6  col-lg-6 col-6 text-right\"><span class=\"hw\"><b>QTY</b>&nbsp;<input type=\"number\" class=\"iqty hw2\" value=\"1\" min=\"1\"></span></div>\n                                    </div>\n                                </div>\n                              <div mbr-buttons class=\"col-xs-12 col-md-12 text-center mbr-section-btn\"><a class=\"btn btn-sm add-to-cart btn-white\" href=\"#add\" data-app-placeholder=\"Type Text\">Add to cart</a>\n                          <a href=\"details.html\" class=\"btn btn-sm btn-white-outline\" data-app-placeholder=\"Type Text\">Details</a></div>\n                            </div>\n                              \n                                </div>\n                    \n                            </div>\n                          <div class=\"col-md-4 mb-3\">\n                                <div class=\"card cart_item\">\n                                    <img class=\"img-fluid\" alt=\"100%x280\" src=\"https://i.imgur.com/yugQN69.jpg\">\n                                    <div class=\"card-body\">\n                                        <h4 class=\"card-title product_name\">HP Pro 1</h4>\n                                        <p class=\"card-text\">If you want a different shop interface design, contact Mr Singa4real on +2349066849761</p>\n\n                                    </div>\n                                  <div class=\"group\" mbr-class=\"{'vatshipblock': addvatshipping2 == 1, 'vatshipnone': addvatshipping2 == 2}\">\n                             <div class=\"d-inline\"><strong>ID:</strong>&nbsp;&nbsp;</div>\n                            <div class=\"d-inline\"><strong><span class=\"product_id\" mbr-text>0012</span></strong></div>\n                            <div class=\"d-inline\">&nbsp;&nbsp;<strong>Vat:</strong>&nbsp;&nbsp;</div>\n             